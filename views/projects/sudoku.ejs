<%- include("../inserts/head.ejs", {stylesheet:"sudoku.css"}) %>
<h1>Sudoku</h1>
<% nums = [1,2,3,4,5,6,7,8,9] %>
<table class="sudoku">
  <% nums.forEach(r => { %>
  <tr>
    <% nums.forEach(c => { %>
    <% let b = 3*Math.floor((r-1)/3)+Math.floor((c-1)/3)+1 %>
    <td id="<%-`${c}${r}`%>" class="<%-`r${r} c${c} b${b}`%> tile hoverable"></td>
    <% }) %>
  </tr>
  <% }) %>
</table>
<button id="solve" class="btn">Solve</button>
<button id="generate" class="btn">Generate</button>
<button id="clear" class="btn">Clear</button>
<%- include("../inserts/foot.ejs", {scriptfile:"sudoku.js"}) %>